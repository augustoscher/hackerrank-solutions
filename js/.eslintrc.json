{
  "env": {
      "browser": true,
      "es2020": true,
      "jest": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "react-hooks"
  ]
}
